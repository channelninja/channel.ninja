{
  "name": "channel.ninja",
  "private": true,
  "version": "1.4.10",
  "description": "",
  "main": "server/dist/main.js",
  "scripts": {
    "start:server": "nest start --watch",
    "start:client": "cd client && react-scripts start",
    "build:nest": "nest build",
    "build:cra": "cd client && react-scripts build",
    "build": "yarn build:nest && yarn build:cra",
    "start:dev": "NODE_ENV=development concurrently -n client,server -c magenta,cyan \"npm run start:client\" \"npm run start:server\"",
    "start:prod": "NODE_ENV=production node dist/main.js",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate -d server/core/database/data-source.prod.ts",
    "migration:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run -d server/core/database/data-source.prod.ts",
    "migration:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert -d server/core/database/data-source.prod.ts",
    "prebuild": "rimraf dist && rimraf client/build",
    "commit": "cz",
    "format": "prettier --write \"{server,client}/**/*.{ts,tsx,css}\"",
    "prepare": "husky install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "author": "",
  "license": "MIT",
  "types": "./server/dist/main.d.ts",
  "dependencies": {
    "@logtail/pino": "^0.3.2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.3.12",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/serve-static": "^3.0.1",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@nestjs/websockets": "^9.3.9",
    "@nicholas.braun/nestjs-redoc": "^2.2.3",
    "@reduxjs/toolkit": "^1.9.3",
    "axios": "^1.3.4",
    "buffer": "^6.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "classnames": "^2.3.2",
    "connect-pg-simple": "^8.0.0",
    "express-session": "^1.17.3",
    "formik": "^2.2.9",
    "lightning": "^7.1.4",
    "nestjs-pino": "^3.1.2",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-aria": "^3.23.1",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-redux": "^8.0.5",
    "react-stately": "^3.21.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "socket.io-client": "^4.6.1",
    "throttle-debounce": "^5.0.0",
